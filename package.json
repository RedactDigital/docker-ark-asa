{
  "name": "typescript-node-bun",
  "version": "1.0.0",
  "module": "app/index.ts",
  "type": "module",
  "main": "dist/app/index.js",
  "scripts": {
    "test": "bun test --watch app/test-setup.ts",
    "test:production": "bun test --timeout 20 --rerun-each 10 --bail 10",
    "clean": "rm -rf dist && rm -rf storage && echo 'Done.'",
    "build": "bun build --target=bun app/index.ts --outfile=dist/index.js",
    "start": "bun --watch app/index.ts",
    "start:worker": "bun --watch worker/index-worker.ts",
    "lint": "prettier --check app && eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore && cspell . --no-progress --unique",
    "format": "prettier --write app"
  },
  "author": "Patrick Rizzardi <patrick@redact.digital> (https://redact.digital)",
  "dependencies": {
    "@elysiajs/cors": "^0.7.1",
    "@elysiajs/jwt": "^0.7.0",
    "@sequelize/core": "alpha",
    "@sequelize/validator.js": "^7.0.0-alpha.33",
    "bull": "^4.11.5",
    "cron": "^3.1.6",
    "datadog-winston": "^1.5.1",
    "dayjs": "^1.11.9",
    "discord.js": "^14.13.0",
    "elysia": "^0.7.21",
    "mathjs": "^11.9.1",
    "mysql2": "^3.6.3",
    "node-steam-openid": "^1.2.0",
    "npm": "^10.2.3",
    "rcon-client": "^4.2.4",
    "steamapi": "^2.4.2",
    "stripe": "^14.4.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@elysiajs/swagger": "^0.7.4",
    "@types/cron": "^2.4.0",
    "@types/datadog-winston": "^1.0.5",
    "@types/node-steam-openid": "^1.0.3",
    "@types/steamapi": "^2.2.5",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "bun-types": "^1.0.3",
    "cspell": "^6.31.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "logform": "^2.5.1",
    "openapi-types": "^12.1.3",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
